archivos = os.listdir('./Archivos/')
print(archivos)

def lista_csv(nombres):
    filtro = []
    for nombre in nombres:
        if nombre[-4:] == ".csv":
            filtro.append(nombre)
    return filtro

archivos = lista_csv(archivos)
print(archivos)















